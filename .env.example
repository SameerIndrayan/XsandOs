# Server Configuration
PORT=3000
NODE_ENV=development

# Mock Mode (set to 1 to use deterministic mock output)
MOCK_MODE=1

# VisionAgents Configuration
VISIONAGENTS_ENABLED=0
VISIONAGENTS_API_KEY=your_visionagents_api_key_here
VISIONAGENTS_API_URL=https://api.visionagents.ai/v1/analyze

# Frame Extraction Configuration
FRAME_INTERVAL_SEC=0.5
FRAME_OUTPUT_DIR=./tmp/frames

# Upload Configuration
UPLOAD_DIR=./uploads

# Gemini API Configuration (used for video analysis and Q&A)
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_MODEL=gemini-1.5-flash

# OpenAI Whisper API Configuration (speech-to-text)
OPENAI_API_KEY=your_openai_api_key_here

# Eleven Labs API Configuration (text-to-speech)
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here
ELEVENLABS_VOICE_ID=TxGEqnHWrfWFTfGW9XjX
ELEVENLABS_MODEL_ID=eleven_turbo_v2_5
